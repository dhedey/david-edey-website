---
import BaseLayout, { type Props as BaseLayoutProps } from './BaseLayout.astro';
import Prose from '../components/Prose.astro';
import SimpleContentLayout from './SimpleContentLayout.astro';

interface Props {
    title: string;
}
const props: Props = Astro.props;
---

<SimpleContentLayout headTitle={`Blog | ${props.title}`} title={props.title} isArticle={true}>
    <slot />
</SimpleContentLayout>
